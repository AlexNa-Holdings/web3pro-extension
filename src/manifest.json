{
  "name": "Web3Pro",
  "version": "0.1.0",
  "manifest_version": 3,
  "description": "Web3Pro",
  "homepage_url": "https://github.com/AlexNa-Holdings/web3pro-extension",
  "background": {
    "service_worker": "index.js",
    "type": "module"
  },
  "action": {
    "default_title": "Web3Pro",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "96": "icon96.png"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "content_scripts": [
    {
      "matches": [
        "file://*/*",
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "frame.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "96": "icon96.png"
  }
}