{
  "name": "Web3Pro",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Web3Pro",
  "homepage_url": "https://github.com/floating/frame",
  "background": {
    "service_worker": "index.js",
    "type": "module"
  },
  "action": {
    "default_title": "Web3Pro",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "96": "icons/icon96.png"
    }
  },
  "content_scripts" : [{
    "matches": ["file://*/*", "http://*/*", "https://*/*"],
    "js": ["inject.js"],
    "run_at": "document_start",
    "all_frames": true
  }],
"permissions": [
  "activeTab",
  "tabs"
],
"host_permissions": [
  "https://*/*",
  "http://*/*"
],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "96": "icons/icon96.png"
  }
}
